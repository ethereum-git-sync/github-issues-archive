    "body": "web3 beta41\r\nGeth/v1.8.22-stable-7fa3509e/linux-amd64/go1.11.5\r\nOr latest unstable dockerimage\r\n\r\nRinkeby network\r\n\r\n    const Web3 = require('web3');\r\n\r\n    if (typeof web3 !== 'undefined') {\r\n        web3 = new Web3(web3.currentProvider);\r\n    } else {\r\n        const wsProvider = new Web3.providers.WebsocketProvider('ws://localhost:8546');\r\n\r\n        wsProvider.on('error', e => console.error('WS Error', e));\r\n        wsProvider.on('end', e => console.error('WS End', e));\r\n        web3 = new Web3(wsProvider);\r\n    }\r\n\r\n    web3.eth.subscribe('newBlockHeaders', (error, blockData) => {\r\n        if(error) console.error(error);\r\n        console.log(blockData);\r\n    });\r\n\r\n    web3.eth.subscribe('pendingTransactions', function (error, transactionHash) {\r\n        if(error) console.error(error);\r\n        console.log(transactionHash);\r\n    });",
